{
	"compilerOptions": {
	  "strict": true,
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "target": "es2019",
	  "lib": ["es2019", "es2020", "es2022.error", "ES6", "DOM"],
	  "removeComments": true,
	  "useUnknownInCatchVariables": false,
	  "forceConsistentCasingInFileNames": true,
	  "noImplicitAny": true,
	  "noImplicitReturns": true,
	  "noUnusedLocals": true,
	  "strictNullChecks": true,
	  "preserveConstEnums": true,
	  "esModuleInterop": true,
	  "resolveJsonModule": true,
	  "incremental": true,
	  "declaration": true,
	  "sourceMap": true,
	  "skipLibCheck": true,
	  "outDir": "./dist",
	  "rootDir": "./",
	  "baseUrl": "./",
	  "paths": {
		"n8n-workflow": ["node_modules/n8n-workflow"],
		"@": ["./src"]
	  },
	  "typeRoots": [
		"node_modules/@types"
	  ],
	  "types": ["node"]
	},
	"include": [
	  "credentials/**/*.ts",
	  "nodes/**/*.ts",
	  "src/**/*.ts"
	],
	"exclude": [
	  "node_modules",
	  "dist",
	  "**/*.spec.ts",
	  "**/*.test.ts"
	]
  }